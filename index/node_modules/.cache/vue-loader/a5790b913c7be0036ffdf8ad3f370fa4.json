;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Brand.vue","sourceRoot":"src/views/pinpai","sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <img src=\"../../assets/images/icon.jpg\" alt=\"\" />\r\n      <div id=\"header\">\r\n        <!-- 头部信息开始 -->\r\n        <div>\r\n          <mt-header fixed class=\"header_one\" v-if=\"scrolls == 1\">\r\n            <router-link slot=\"left\" to=\"/\">\r\n              <mt-button icon=\"back\"></mt-button>\r\n            </router-link>\r\n            <div slot=\"right\">\r\n              <mt-button icon=\"more\" type=\"primary\">\r\n                <img src=\"../../assets/images/cart_enabled.png\" slot=\"icon\" />\r\n              </mt-button>\r\n            </div>\r\n          </mt-header>\r\n\r\n          \r\n          <mt-header fixed class=\"header_tow\" title=\"品牌馆\" v-else>\r\n            <router-link slot=\"left\" to=\"/\">\r\n              <mt-button icon=\"back\"></mt-button>\r\n            </router-link>\r\n            <div slot=\"right\">\r\n              <mt-button icon=\"more\" type=\"primary\">\r\n                <img src=\"../../assets/images/cart_disabled.png\" slot=\"icon\" />\r\n              </mt-button>\r\n            </div>\r\n          </mt-header>\r\n        </div>\r\n        <!-- 头部信息结束 -->\r\n\r\n        <!-- 头部导航开始 -->\r\n        <div>\r\n          <mt-navbar v-model=\"active\" class=\"navbar\">\r\n            <mt-tab-item id=\"1\">机构品牌</mt-tab-item>\r\n            <mt-tab-item id=\"2\">材料品牌</mt-tab-item>\r\n          </mt-navbar>\r\n        </div>\r\n        <!-- 头部导航结束 -->\r\n\r\n        <!-- 中间导航开始 -->\r\n        <div class=\"daohang\">\r\n          <mt-tab-container v-model=\"active\">\r\n            <!-- 机构的品牌开始 -->\r\n            <mt-tab-container-item class=\"container\" id=\"1\">\r\n              <span v-for=\"(v, k) of articles\" :key=\"k\">\r\n                <router-link to=\"/\">{{ v }}</router-link>\r\n              </span>\r\n            </mt-tab-container-item>\r\n            <!-- 机构的品牌结束 -->\r\n\r\n            <!-- 材料的品牌开始 -->\r\n            <mt-tab-container-item class=\"container\" id=\"2\">\r\n              <span v-for=\"(v, k) of article\" :key=\"k\">\r\n                <router-link to=\"/\">{{ v }}</router-link>\r\n              </span>\r\n            </mt-tab-container-item>\r\n            <!-- 材料的品牌结束 -->\r\n          </mt-tab-container>\r\n        </div>\r\n        <!-- 中间导航结束 -->\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 正文内容开始 -->\r\n\r\n    <div class=\"question_one\">\r\n      <mt-tab-container v-model=\"active\">\r\n        <mt-tab-container-item id=\"1\">\r\n          <!-- 一 -->\r\n          <div id=\"question\" class=\"question_tow\">\r\n            <div class=\"question\">\r\n              <!-- 品牌log开始 -->\r\n              <div>\r\n                <img\r\n                  src=\"../../assets/images/taikang_1.png\"\r\n                  class=\"question_img\"\r\n                />\r\n              </div>\r\n              <!-- 品牌信息开始 -->\r\n              <div>\r\n                <div class=\"question_top\">\r\n                  <p>拜博口腔</p>\r\n                  <router-link to=\"/\" class=\"question_a\"\r\n                    >品牌专场<span class=\"icon\"></span\r\n                  ></router-link>\r\n                </div>\r\n                <div class=\"span\">\r\n                  <span>214家机构</span\r\n                  >&nbsp;|&nbsp;<span>52个城市</span>&nbsp;|&nbsp;<span\r\n                    >200位医生</span\r\n                  >\r\n                </div>\r\n              </div>\r\n              <!-- 品牌信息结束 -->\r\n            </div>\r\n            <div class=\"jianjie\">\r\n              <div>\r\n                <!-- 品牌简介开始 -->\r\n                <div><span class=\"icon_one\"></span>品牌简介</div>\r\n                <div>\r\n                  拜博口腔医疗集团创建于1933年，历经26年的积累和发展，在北京、上海、广州、深圳等50余个城市拥有200余家口腔医疗机构，是布局全国、规模化运营的口腔医疗集团。\r\n                </div>\r\n                <!-- 品牌简介结束 -->\r\n              </div>\r\n\r\n              <div class=\"lianjie\">\r\n                <!-- 诊断价格开始 -->\r\n                <router-link to=\"/\">\r\n                  <div>\r\n                    <img src=\"../../assets/images/taikang_2.png\" alt=\"\" />\r\n                  </div>\r\n                </router-link>\r\n\r\n                <div>\r\n                  <router-link to=\"/\">\r\n                    <div>\r\n                      <img src=\"../../assets/images/taikang_3.png\" alt=\"\" />\r\n                    </div>\r\n                    <div>\r\n                      <div class=\"neirong\">\r\n                        [金属托槽矫正]&nbsp;[26年专&nbsp;·&nbsp;·&nbsp;·\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"jiage\">\r\n                      <span>特</span><span>¥18000</span><span>¥23000</span>\r\n                    </div>\r\n                  </router-link>\r\n\r\n                  <router-link to=\"/\">\r\n                    <div>\r\n                      <img src=\"../../assets/images/taikang_4.png\" alt=\"\" />\r\n                    </div>\r\n                    <div>\r\n                      <div class=\"neirong\">\r\n                        [隐形矫正]&nbsp;[田园院长亲诊&nbsp;·&nbsp;·&nbsp;·\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"jiage\">\r\n                      <span>特</span><span>¥28000</span><span>¥30000</span>\r\n                    </div>\r\n                  </router-link>\r\n\r\n                  <router-link to=\"/\">\r\n                    <div>\r\n                      <img src=\"../../assets/images/taikang_5.png\" alt=\"\" />\r\n                    </div>\r\n                    <div>\r\n                      <div class=\"neirong\">\r\n                        [隐形托槽矫正]&nbsp;进口orm&nbsp;·&nbsp;·&nbsp;·\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"jiage\">\r\n                      <span>特</span><span>¥9800</span><span>¥21500</span>\r\n                    </div>\r\n                  </router-link>\r\n                </div>\r\n                <!-- 诊断价格结束 -->\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!-- 二 -->\r\n          <div id=\"question\" class=\"question_tow\">\r\n            <div class=\"question\">\r\n              <!-- 品牌log开始 -->\r\n              <div>\r\n                <img\r\n                  src=\"../../assets/images/weien_1.png\"\r\n                  class=\"question_img\"\r\n                />\r\n              </div>\r\n              <!-- 品牌信息开始 -->\r\n              <div>\r\n                <div class=\"question_top\">\r\n                  <p>维恩口腔</p>\r\n                  <router-link to=\"/\" class=\"question_a\"\r\n                    >品牌专场<span class=\"icon\"></span\r\n                  ></router-link>\r\n                </div>\r\n                <div class=\"span\">\r\n                  <span>9家机构</span\r\n                  >&nbsp;|&nbsp;<span>4个城市</span>&nbsp;|&nbsp;<span\r\n                    >20位医生</span\r\n                  >\r\n                </div>\r\n              </div>\r\n              <!-- 品牌信息结束 -->\r\n            </div>\r\n            <div class=\"jianjie\">\r\n              <div>\r\n                <!-- 品牌简介开始 -->\r\n                <div><span class=\"icon_one\"></span>品牌简介</div>\r\n                <div>\r\n                  维恩口腔成立于2006年，是一家连锁品牌机构，主要分布于北京6家、杭州1家、绍兴1家；始终秉承“真材实料，医者仁心”的行医理念，是专业提供先进齿科服务口腔品牌，长期以来提供专业的齿科服务。拥有舒适儿牙中心、数字化种植中心、数字化正畸中心等。\r\n                </div>\r\n                <!-- 品牌简介结束 -->\r\n              </div>\r\n\r\n              <div class=\"lianjie\">\r\n                <!-- 诊断价格开始 -->\r\n                <router-link to=\"/\">\r\n                  <div>\r\n                    <img src=\"../../assets/images/weien_2.png\" alt=\"\" />\r\n                  </div>\r\n                </router-link>\r\n\r\n                <div>\r\n                  <router-link to=\"/\">\r\n                    <div>\r\n                      <img src=\"../../assets/images/weien_3.png\" alt=\"\" />\r\n                    </div>\r\n                    <div>\r\n                      <div class=\"neirong\">\r\n                        [金属托槽矫正]&nbsp;[北大博士团队&nbsp;·&nbsp;·&nbsp;·\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"jiage\">\r\n                      <span>特</span><span>¥7999</span><span>¥10000</span>\r\n                    </div>\r\n                  </router-link>\r\n\r\n                  <router-link to=\"/\">\r\n                    <div>\r\n                      <img src=\"../../assets/images/weien_4.png\" alt=\"\" />\r\n                    </div>\r\n                    <div>\r\n                      <div class=\"neirong\">\r\n                        [种植牙]&nbsp;[让每一位老百姓都能&nbsp;·&nbsp;·&nbsp;·\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"jiage\">\r\n                      <span>特</span><span>¥4980</span><span>¥6800</span>\r\n                    </div>\r\n                  </router-link>\r\n\r\n                  <router-link to=\"/\">\r\n                    <div>\r\n                      <img src=\"../../assets/images/weien_5.png\" alt=\"\" />\r\n                    </div>\r\n                    <div>\r\n                      <div class=\"neirong\">\r\n                        [隐形托槽矫正]&nbsp;[北大博士团队&nbsp;·&nbsp;·&nbsp;·\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"jiage\">\r\n                      <span>特</span><span>¥9800</span><span>¥21500</span>\r\n                    </div>\r\n                  </router-link>\r\n                </div>\r\n                <!-- 诊断价格结束 -->\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </mt-tab-container-item>\r\n\r\n        <mt-tab-container-item id=\"2\">\r\n          <!-- 一 -->\r\n          <div id=\"meterial\" class=\"meterial\">\r\n            <div>\r\n              <img src=\"../../assets/images/zhengya_1.png\" />\r\n            </div>\r\n            <div class=\"meterial_one\">\r\n              <!-- 品牌log开始 -->\r\n              <div>\r\n                <div>\r\n                  <img\r\n                    src=\"../../assets/images/zhengya_2.png\"\r\n                    class=\"meterial_img\"\r\n                  />\r\n                </div>\r\n                <!-- 品牌信息开始 -->\r\n                <div>\r\n                  <div class=\"meterial_router\">\r\n                    <p>正雅</p>\r\n                    <router-link to=\"/\" class=\"meterial_a\"\r\n                      >了解品牌<span class=\"icon\"></span\r\n                    ></router-link>\r\n                  </div>\r\n                  <div class=\"meterial_span\">\r\n                    <span></span><span>中国</span>&nbsp;|&nbsp;<span\r\n                      >牙齿矫正品牌</span\r\n                    >\r\n                  </div>\r\n                </div>\r\n                <!-- 品牌信息结束 -->\r\n              </div>\r\n\r\n              <div class=\"meterial_jianjie\">\r\n                <div>\r\n                  <!-- 品牌简介开始 -->\r\n                  <div><span class=\"icon_one\"></span>品牌简介</div>\r\n                  <div>\r\n                    Smartee隐形矫治通过计算机三维成像，对错颌畸形模型进行模拟矫正，在模拟牙齿移动过程中制作一系列上下牙列的定制式隐形正畸矫治器。利用定制式隐形正畸矫治器的弹性，每两周更换一副，逐渐矫正错颌的牙齿，最终实现牙齿的美丽蜕变。\r\n                  </div>\r\n                  <!-- 品牌简介结束 -->\r\n                </div>\r\n\r\n                <div class=\"meterial_zhanshi\">\r\n                  <!-- 效果展示开始 -->\r\n                  <div>\r\n                    <span class=\"icon_tow\"></span>\r\n                    <span>效果展示</span>\r\n\r\n                    <div>\r\n                      <img src=\"../../assets/images/zhengya_3.png\" />\r\n                      <img src=\"../../assets/images/zhengya_4.png\" />\r\n                    </div>\r\n                  </div>\r\n                  <!-- 效果展示结束 -->\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!-- 二 -->\r\n          <div id=\"meterial\" class=\"meterial\">\r\n            <div>\r\n              <img src=\"../../assets/images/mei_1.png\" />\r\n            </div>\r\n            <div class=\"meterial_one\">\r\n              <!-- 品牌log开始 -->\r\n              <div>\r\n                <div>\r\n                  <img\r\n                    src=\"../../assets/images/mei_2.png\"\r\n                    class=\"meterial_img\"\r\n                  />\r\n                </div>\r\n                <!-- 品牌信息开始 -->\r\n                <div>\r\n                  <div class=\"meterial_router\">\r\n                    <p>美立刻</p>\r\n                    <router-link to=\"/\" class=\"meterial_a\"\r\n                      >了解品牌<span class=\"icon\"></span\r\n                    ></router-link>\r\n                  </div>\r\n                  <div class=\"meterial_span\">\r\n                    <span></span><span>中国</span>&nbsp;|&nbsp;<span\r\n                      >牙齿矫正品牌</span\r\n                    >\r\n                  </div>\r\n                </div>\r\n                <!-- 品牌信息结束 -->\r\n              </div>\r\n\r\n              <div class=\"meterial_jianjie\">\r\n                <div>\r\n                  <!-- 品牌简介开始 -->\r\n                  <div><span class=\"icon_one\"></span>品牌简介</div>\r\n                  <div>\r\n                    美立刻隐形矫治器是由来自清华、北大的口腔